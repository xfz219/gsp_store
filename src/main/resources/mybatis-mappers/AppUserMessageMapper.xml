<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.puhui.app.dao.AppUserMessageDao">

	<resultMap id="appUserMessageObj" type="appUserMessage">
		<id property="id" column="id"/>
		<result property="sellerNumber" column="seller_number"/>
		<result property="appLendRequestId" column="app_lend_request_id"/>
		<result property="context" column="context"/>
		<result property="isRead" column="is_read"/>
		<result property="messageType" column="message_type"/>
		<result property="createTime" column="create_time"/>
		<result property="mobile" column="mobile"/>
	</resultMap>
	
	<insert id="insertUserMessage" parameterType="appUserMessage" useGeneratedKeys="true" keyProperty="id">
		insert into app_user_message 
		(seller_number,context,is_read,message_type,create_time,update_time) 
		values 
		(#{sellerNumber},#{context},#{isRead},#{messageType},#{createTime},#{updateTime})
	</insert>
</mapper> 
